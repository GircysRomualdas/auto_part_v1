<div>
  <h2>Manage Sellers</h2>
  <table class="table table-hover">
    <thead>
      <tr>
        <th>ID</th>
        <th>Username</th>
        <th>Email</th>
        <th>Created</th>
        <th>Updated</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @sellers.each do |seller| %>
        <tr>
          <td><%= seller.id %></td>
          <td><%= seller.username %></td>
          <td><%= seller.email %></td>
          <td><%= seller.created_at %></td>
          <td><%= seller.updated_at %></td>
          <td>
            <%= button_to "Delete", manage_seller_path(seller), class: "btn btn-danger", method: :delete, data: {turbo_confirm: "Are you sure?"} %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>